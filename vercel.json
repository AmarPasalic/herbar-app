{
    "version": 2,
    "functions": {
        "api/index.js": {
            "runtime": "nodejs20.x",
            "memory": 512,
            "maxDuration": 10
        }
    },
    "env": {
        "NODE_ENV": "production"
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/uploads/(.*)",
            "dest": "/api/index.js"
        }
    ]
}